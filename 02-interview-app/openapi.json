{
  "openapi": "3.1.0",
  "info": {
    "title": "Collaborative Code App API",
    "description": "Backend API for collaborative code editor",
    "version": "0.1.0"
  },
  "paths": {
    "/health": {
      "get": {
        "summary": "Health check",
        "operationId": "health_check_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": true,
                  "type": "object",
                  "title": "Response Health Check Health Get"
                }
              }
            }
          }
        }
      }
    },
    "/rooms": {
      "get": {
        "summary": "List all rooms",
        "operationId": "list_rooms_rooms_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RoomListResponse"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create a new room",
        "operationId": "create_room_rooms_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RoomCreateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RoomCreateResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/rooms/{room_id}": {
      "get": {
        "summary": "Get room state",
        "operationId": "get_room_rooms__room_id__get",
        "parameters": [
          {
            "name": "room_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Room Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RoomState"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "patch": {
        "summary": "Update existing room",
        "operationId": "update_room_rooms__room_id__patch",
        "parameters": [
          {
            "name": "room_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Room Id"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RoomUpdateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RoomState"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "RoomCreateRequest": {
        "properties": {
          "code": {
            "type": "string",
            "title": "Code",
            "description": "Optional initial code",
            "default": ""
          },
          "language": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Language",
            "description": "Optional initial language"
          }
        },
        "type": "object",
        "title": "RoomCreateRequest"
      },
      "RoomCreateResponse": {
        "properties": {
          "room_id": {
            "type": "string",
            "title": "Room Id",
            "description": "Unique room identifier"
          },
          "code": {
            "type": "string",
            "title": "Code",
            "description": "Shared code in the room",
            "default": ""
          },
          "language": {
            "type": "string",
            "enum": [
              "python",
              "javascript",
              "typescript",
              "sql",
              "java",
              "c",
              "cpp"
            ],
            "title": "Language",
            "description": "Programming language",
            "default": "python"
          },
          "last_updated": {
            "type": "number",
            "title": "Last Updated",
            "description": "Unix timestamp of last update"
          }
        },
        "type": "object",
        "required": [
          "room_id"
        ],
        "title": "RoomCreateResponse"
      },
      "RoomListResponse": {
        "properties": {
          "rooms": {
            "additionalProperties": {
              "$ref": "#/components/schemas/RoomState"
            },
            "type": "object",
            "title": "Rooms"
          }
        },
        "type": "object",
        "required": [
          "rooms"
        ],
        "title": "RoomListResponse"
      },
      "RoomState": {
        "properties": {
          "room_id": {
            "type": "string",
            "title": "Room Id",
            "description": "Unique room identifier"
          },
          "code": {
            "type": "string",
            "title": "Code",
            "description": "Shared code in the room",
            "default": ""
          },
          "language": {
            "type": "string",
            "enum": [
              "python",
              "javascript",
              "typescript",
              "sql",
              "java",
              "c",
              "cpp"
            ],
            "title": "Language",
            "description": "Programming language",
            "default": "python"
          },
          "last_updated": {
            "type": "number",
            "title": "Last Updated",
            "description": "Unix timestamp of last update"
          }
        },
        "type": "object",
        "required": [
          "room_id"
        ],
        "title": "RoomState"
      },
      "RoomUpdateRequest": {
        "properties": {
          "code": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Code",
            "description": "New code contents"
          },
          "language": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Language",
            "description": "New language"
          }
        },
        "type": "object",
        "title": "RoomUpdateRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}